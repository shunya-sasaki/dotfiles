{
  "home.nix": {
    "prefix": "home.nix",
    "description": "home.nix template",
    "body": [
      "{ config, pkgs, ...}:",
      "",
      "{",
      "  nixpkgs.config.allowUnfree = true;",
      "  fonts.fontconfig.enable = true;",
      "",
      "  home.username = \"${1:username}\";",
      "  home.homeDirectory = \"${2:homeDirectory}\";",
      "  home.stateVersion = \"25.11\";",
      "  home.packages = import ./packages.nix { inherit pkgs; };",
      "  programs = import ./programs.nix;",
      "}"
    ]
  },
  "flake-rust": {
    "prefix": "flake-rust",
    "description": "flake.nix for Rust project",
    "body": [
      "{",
      "  description = \"A devShell example\";",
      "",
      "  inputs = {",
      "    nixpkgs.url = \"github:NixOS/nixpkgs/nixos-unstable\";",
      "    rust-overlay.url = \"github:oxalica/rust-overlay\";",
      "    flake-utils.url = \"github:numtide/flake-utils\";",
      "  };",
      "",
      "  outputs =",
      "    {",
      "      self,",
      "      nixpkgs,",
      "      rust-overlay,",
      "      flake-utils,",
      "      ...",
      "    }:",
      "    flake-utils.lib.eachDefaultSystem (",
      "      system:",
      "      let",
      "        overlays = [ (import rust-overlay) ];",
      "        pkgs = import nixpkgs {",
      "          inherit system overlays;",
      "        };",
      "        codelldb = pkgs.vscode-extensions.vadimcn.vscode-lldb;",
      "      in",
      "      {",
      "        devShells.default =",
      "          with pkgs;",
      "          mkShell {",
      "            buildInputs = [",
      "              openssl",
      "              pkg-config",
      "              eza",
      "              fd",
      "              codelldb",
      "              (rust-bin.beta.latest.default.override {",
      "                extensions = [",
      "                  \"rust-src\"",
      "                  \"rust-analyzer\"",
      "                  \"rustfmt\"",
      "                  \"clippy\"",
      "                ];",
      "              })",
      "            ];",
      "",
      "            shellHook = ''",
      "              alias ls=eza",
      "              alias find=fd",
      "              codelldb_path=\\${codelldb}/share/vscode/extensions/vadimcn.vscode-lldb/adapter/codelldb",
      "              if [ ! -d \".bin\" ]; then",
      "                  mkdir .bin",
      "              fi",
      "              echo \"/*\" > .bin/.gitignore",
      "              ln -sf \"\\$codelldb_path\" ./.bin/codelldb",
      "            '';",
      "          };",
      "      }",
      "    );",
      "}"
    ]
  },
  "flake-python": {
    "prefix": "flake-python",
    "description": "flake.nix for Python project",
    "body": [
      "{",
      "  description = \"Python development environment\";",
      "",
      "  inputs = {",
      "    nixpkgs.url = \"github:NixOS/nixpkgs/nixos-unstable\";",
      "    flake-utils.url = \"github:numtide/flake-utils\";",
      "    nixpkgs-python.url = \"github:cachix/nixpkgs-python\";",
      "  };",
      "",
      "  outputs =",
      "    {",
      "      self,",
      "      nixpkgs,",
      "      nixpkgs-python,",
      "      flake-utils,",
      "      ...",
      "    }:",
      "    flake-utils.lib.eachDefaultSystem (",
      "      system:",
      "      let",
      "        pkgs = nixpkgs.legacyPackages.\\${system};",
      "        pythonVersion = \"${1:pythonVersion}\";",
      "        python = nixpkgs-python.packages.\\${system}.\\${pythonVersion};",
      "      in",
      "      {",
      "        devShells.default =",
      "          with pkgs;",
      "          mkShell {",
      "              packages = [",
      "              python",
      "              pkgs.uv",
      "              ];",
      "              LD_LIBRARY_PATH = pkgs.lib.makeLibraryPath [",
      "                pkgs.stdenv.cc.cc",
      "                pkgs.zlib",
      "              ];",
      "              shellHook = ''",
      "                echo \"Python \\${pythonVersion} environment with uv\"",
      "                export UV_PYTHON=\\${python}/bin/python",
      "                if [ ! -d \".venv\" ]; then",
      "                    echo \"Createing virtual environment...\"",
      "                    uv venv",
      "                fi",
      "                source .venv/bin/activate",
      "            '';",
      "          };",
      "      }",
      "    );",
      "}"
    ]
  },
  "flake-node": {
    "prefix": "flake-node",
    "description": "flake.nix for Node.js project",
    "body": [
      "{",
      "  description = \"Node.js development environment\";",
      "",
      "  inputs = {",
      "    nixpkgs.url = \"github:NixOS/nixpkgs/nixos-unstable\";",
      "    flake-utils.url = \"github:numtide/flake-utils\";",
      "  };",
      "",
      "  outputs =",
      "    {",
      "      self,",
      "      nixpkgs,",
      "      flake-utils,",
      "      ...",
      "    }:",
      "    flake-utils.lib.eachDefaultSystem (",
      "      system:",
      "      let",
      "        pkgs = nixpkgs.legacyPackages.\\${system};",
      "        nodeVersion = pkgs.nodejs_${1:22};",
      "      in",
      "      {",
      "        devShells.default =",
      "          with pkgs;",
      "          mkShell {",
      "            packages = [",
      "              nodeVersion",
      "              nodeVersion.pkgs.yarn",
      "              nodeVersion.pkgs.pnpm",
      "            ];",
      "            shellHook = ''",
      "              echo \"Node.js \\${nodeVersion.version} environment\"",
      "            '';",
      "          };",
      "      }",
      "    );",
      "}"
    ]
  }
}
