  },
  "Mermaid Sequence Group": {
    "prefix": "mmd-sequence-create-participant",
    "body": "create ${1: var}"
  },
  "Mermaid Sequence Message": {
    "prefix": "mmd-sequence-message",
    "description": "Sequence message",
    "body": "${1:actor} ${2|->,-->,->>,-->>,<<->>,<<-->>,-x,--x,-),--)|}: ${3:message}"
  },
