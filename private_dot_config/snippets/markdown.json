{
  "Shields.io Static Badge": {
    "prefix": "md-shields-static",
    "description": [
      "Insert a static badge from Shields.io.",
      "badgeName: The name of the badge and right side text.",
      "rightBgColor: The background color of the right side of the badge.",
      "logo: The logo to use for the badge.",
      "leftBgColor: The background color of the left side of the badge.",
      "logoColor: The color of the logo."
    ],
    "body": [
      "![${1:badgeName}](https://img.shields.io/badge/${1:badgeName}-${2:gray}?logo=${1:badgeName}&labelColor=${3:gray}&logoColor=${4:white})"
    ]
  },
  "Shields.io GitHub License": {
    "prefix": "md-shields-license",
    "description": [
      "Insert a license badge from Shields.io.",
      "user: The GitHub username.",
      "repo: The repository name."
    ],
    "body": [
      "![${1:badgeName}](https://img.shields.io/github/license/${2:user}/${3:repo})"
    ]
  },
  "README": {
    "prefix": "md-readme",
    "description": "Template of README.md",
    "body": [
      "# ${1: Project Title}",
      "",
      "## üì¶ Requirements",
      "",
      "## ‚öôÔ∏è Setup",
      "",
      "## üöÄ Usage",
      "",
      "## üìö Reference",
      "",
      "## üìÑ License",
      "",
      "## üîó Related Projects",
      ""
    ]
  },
  "Author": {
    "prefix": "md-author",
    "description": "Author for README.md",
    "body": [
      "## Author",
      "",
      "- ${1:Author Name}",
      "  - Mail: [${2:author.email@example.com}](mailto:${2:author.email@example.com})",
      "  - X: [@${3:AuthorX}](https://x.com/${3:AuthorX})"
    ]
  },
  "Link to Iconify": {
    "prefix": "md-link-iconify",
    "description": [
      "Link to Iconify for more icons.",
      "https://code.iconify.design/iconify-icon/3.0.0/iconify-icon.min.js"
    ],
    "body": "<script src=\"https://code.iconify.design/iconify-icon/3.0.0/iconify-icon.min.js\"></script>"
  },
  "Insert Iconify Icon": {
    "prefix": "md-iconify",
    "description": "Insert an Iconify icon.",
    "body": "<iconify-icon icon=\"${1:icon-name}\" style=\"vertical-align: middle;\"></iconify-icon>"
  },
  "Link to FontAwesome": {
    "prefix": "md-link-fontawesome",
    "description": "Link to FontAwesome for more icons.",
    "body": "<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/7.0.0/css/all.min.css\" integrity=\"sha512-DxV+EoADOkOygM4IR9yXP8Sb2qwgidEmeqAEmDKIOfPRQZOWbXCzLC6vjbZyy0vPisbH2SyW27+ddLVCN+OMzQ==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\" />"
  },
  "Insert FontAwesome Icon": {
    "prefix": "md-fontawesome",
    "description": "Insert a FontAwesome icon.",
    "body": "<i class=\"fa fa-${1:icon-name}\"></i>"
  },
  "Link to Devicon": {
    "prefix": "md-link-devicon",
