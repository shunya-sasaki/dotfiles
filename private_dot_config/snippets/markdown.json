  "Mermaid Sequence Participant": {
    "prefix": "mmd-sequence-participant",
    "body": "participant ${1: var} as ${2: name}"
  },
  "Mermaid Sequence Actor": {
    "prefix": "mmd-sequence-group",
    "body": [
      "box ${1: group_name}",
      "",
      "end"
    ]
  },
  "Mermaid Sequence Create": {
    "prefix": "mmd-sequence-create-participant",
    "body": "create ${1|participant,actor|} ${2: var} as ${3: name}"
  },
  "Mermaid Sequence Destroy": {
    "prefix": "mmd-sequence-create-participant",
