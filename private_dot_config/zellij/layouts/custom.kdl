layout {
    tab hide_floating_panes=true {
        pane focus=true
        pane size=1 borderless=true {
            plugin location="compact-bar"
        }
        floating_panes {
            pane {
                name "Floating pane"
                x "10%"
                y "10%"
                width "80%"
                height "80%"
            }
        }
    }
}

